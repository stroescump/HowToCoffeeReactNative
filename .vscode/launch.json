{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Expo (Hermes)",
      "type": "reactnative",
      "request": "attach",
      "cwd": "${workspaceFolder}",
      "port": 8081,
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///./*": "${workspaceFolder}/*",
        "metro:///./*": "${workspaceFolder}/*",
        "*": "${workspaceFolder}/*"
      }
    }
  ]
}